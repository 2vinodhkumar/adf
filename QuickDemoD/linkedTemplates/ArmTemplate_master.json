{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "QuickDemoD"
		},
		"AzureBlobStorage1_Tenders_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1_Tenders'"
		},
		"notices_rest_lnkserv_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tenders.guru/api/es/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "QuickDemoD_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_Tenders_connectionString": {
						"value": "[parameters('AzureBlobStorage1_Tenders_connectionString')]"
					},
					"notices_rest_lnkserv_properties_typeProperties_url": {
						"value": "[parameters('notices_rest_lnkserv_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}